<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://www.erissoft.com/letic" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <app>
        <delayed_start>
            <enable>no</enable>
            <start offset="720">SUNSET</start>
            <location>
                <latitude>30.111111</latitude>
                <longitude>-70.222222</longitude>
                <time_zone>America/New_York</time_zone>
            </location>
        </delayed_start>
        <lights>
            <max_brightness>0xCC</max_brightness>
            <string name="high_roof" desc="High Roof" pin="5" count="50"/>
            <string name="low_roof" desc="Low Roof" pin="6" count="50"/>
            <string name="bush" desc="Round Bush" pin="7" count="50"/>
            <string name="tree1" desc="Left Tree" pin="14" count="49"/>
            <string name="tree2" desc="Middle Tree" pin="15" count="50"/>
            <string name="tree3" desc="Right Tree" pin="16" count="50"/>
            <string name="ground" desc="Ground Line" pin="17" count="50"/>
            <string name="round_tree" desc="Round Tree" pin="18" count="49"/>
            <string name="door" desc="Door Frame Line" pin="19" count="50"/>
        </lights>
        <writer>
            <protocol>native</protocol>
            <hostname>arduino1</hostname>
            <port>9900</port>
            <!-- delay between port writes in usec -->
            <port_delay>920</port_delay>
            <!-- overhead delay in usec -->
            <send_delay>220</send_delay>
            <byte_send_delay>12</byte_send_delay>
        </writer>
        <e131>
            <sender_uuid>5aa1ded5-b6b7-4334-9f2a-27fdd24807fe</sender_uuid>
            <source_name>LATIC</source_name>
        </e131>
