        <loggers>
            <!-- 
    - each individual library or library group 
    - can have its own output level set 
    - in <subloggers/> section
    -->
            <subloggers>
                <!-- application (non library) messages -->
                <logger name="erissoft.app">
                    <output_level>ALL</output_level>
                </logger>
                <!-- all libraries -->
                <logger name="erissoft.lib"/>
                <!-- common libraries -->
                <logger name="erissoft.lib.com"/>
                <!-- application specific libraries -->
                <logger name="erissoft.lib.app"/>
                <!-- individual library (example) -->
                <logger name="erissoft.lib.com.base"/>
                <logger name="erissoft.lib.app.engine">
                    <output_level>INFO</output_level>
                </logger>
                <logger name="erissoft.lib.app.app">
                    <output_level>INFO</output_level>
                </logger>
            </subloggers>
            <output_level>ALL</output_level>
            <!-- Log4J format string-->
            <format_string>%d{yyyy-MM-dd HH:mm:ss} %-5p - %m at (%F:%L)%n</format_string>
